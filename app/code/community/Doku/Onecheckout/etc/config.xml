        </payment>
        <onecheckout>
            <settings>
                <config_environment>STAGING</config_environment>
                <config_mallid>1234</config_mallid>
                <config_sharedkey>abcdefghijklmnop</config_sharedkey>
                <config_chainnumber>NA</config_chainnumber>
                <config_timelimit_paynow>40</config_timelimit_paynow>
                <config_timelimit_paylater>180</config_timelimit_paylater>
            </settings>
            <info>
                <notifyurl>oco/index/notify</notifyurl>
                <redirectbackurl>oco/index/result</redirectbackurl>
                <identifyurl>oco/index/identify</identifyurl>
                <reviewurl>oco/index/review</reviewurl>
                <inquiryurl>oco/index/inquiry</inquiryurl>
            </info>
        </onecheckout>
    </default>
	<crontab>
		<jobs>
			<my_cron>
				<schedule>
					<cron_expr>*/5 * * * *</cron_expr>
				</schedule>
				<run>
					<model>onecheckout/observer::checkstatus</model>
				</run>
			</my_cron>
		</jobs>
	</crontab>
</config>