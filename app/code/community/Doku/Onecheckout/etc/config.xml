<?xml version="1.0"?>
<config>
	<modules>
		<Doku_Onecheckout>
			<version>0.0.1</version>
		</Doku_Onecheckout>
	</modules>
	<global>
        <resources>
            <onecheckout_setup>
                <setup>
                    <module>Doku_Onecheckout</module>
                </setup>
            </onecheckout_setup>
        </resources>
		<helpers>
			<onecheckout>
				<class>Doku_Onecheckout_Helper</class>
			</onecheckout>
		</helpers>
        <models>
            <onecheckout>
                <class>Doku_Onecheckout_Model</class>
				<resourceModel>onecheckout_resource</resourceModel>
            </onecheckout>
			<onecheckout_resource>
				<class>Doku_Onecheckout_Model_Resource</class>
				<entities>
					<transactions>
						<table>onecheckout_transactions</table>
					</transactions>
				</entities>
			</onecheckout_resource>
        </models>
        <blocks>
            <onecheckout>
                <class>Doku_Onecheckout_Block</class>
            </onecheckout>
        </blocks>
<!--
        <template>
            <email>
                <onecheckout_notifyemail>
                    <label>Transaction Notification</label>
                    <file>onecheckcout_activateemail.html</file>
                    <type>html</type>
                </onecheckout_notifyemail>
            </email>
        </template> -->
        <payment>
            <groups>
                <dokuoco>Onecheckout</dokuoco>
            </groups>
        </payment>
	</global>
    <frontend>
        <routers>
            <onecheckout>
                <use>standard</use>
                <args>
                    <module>Doku_Onecheckout</module>
                    <frontName>oco</frontName>
                </args>
            </onecheckout>
        </routers>
        <layout>
            <updates>
                <onecheckout>
                    <file>onecheckout.xml</file>
                </onecheckout>
            </updates>
        </layout>
    </frontend>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <onecheckout after="Mage_Adminhtml">Doku_Onecheckout</onecheckout>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <layout>
            <updates>
                <onecheckout>
                    <file>onecheckout.xml</file>
                </onecheckout>
            </updates>
        </layout>
    </adminhtml>
    <default>
        <payment>
            <onecheckout_all>
                <group>dokuoco</group>
                <active>0</active>
                <model>onecheckout/allchannel</model>
